


<div class="dropdown">
  <button class="btn btn-secondary dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false">
    Lanches
  </button>
  <ul class="dropdown-menu">

        @foreach(var categoria in Model)
        {
            <li><a class="dropdown-item" asp-controller="Lanche" asp-action="List" style="color:black" asp-route-categoria="@categoria.CategoriaNome">@categoria.CategoriaNome</a></li>
        }

    
    <li><a class="dropdown-item" asp-controller="Lanche" asp-action="List" asp-route-categoria="" style="color:black">Todos os Lanches</a></li>
  </ul>
</div>
